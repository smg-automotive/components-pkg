{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>smg-automotive/renovate-config"
  ],
  "packageRules": [
    {
      "groupName": "UI dependencies",
      "matchUpdateTypes": [
        "patch",
        "minor"
      ],
      "matchPackagePatterns": [
        "@emotion/*",
        "framer-motion",
        "embla-carousel-react",
        "use-debounce"
      ]
    }
  ]
}
